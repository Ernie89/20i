    $services_api = new \TwentyI\API\Services($general_api_key);

    $request = $services_api->postWithFields("/vps/{vpsId}/rebuild", [
            "ApplicationId" => "string",
            "cpanel" => true,
            "cpanelCode" => true,
            "VpsOsId" => "string",
    ]);
