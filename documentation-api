                "ftp" => [
                    "user" => [
                        "Username" => "string",
                        "Domain" => "string",
                        "Password" => "string",
                        "JailFrom" => "string"
                    ]
                ]
            ],
            "update" => [
                "ftp" => [
                    "id" => "string",
                    "user" => [
                        "Password" => "string",
                        "JailFrom" => "string",
                        "UnlockedUntil" => "string",
                        "Enabled" => true
                    ],
                    "acl" => "string"
                ]
            ],
            "delete" => [
                "ftp" => [
                    "id" => ["string"]
                ]
            ]
    ]);
